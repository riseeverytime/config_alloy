{
    "name": "eslint-config-alloy",
    "version": "1.4.2",
    "description": "AlloyTeam ESLint 规则",
    "main": "index.js",
    "scripts": {
        "start": "npm test && npm run site",
        "build": "ts-node scripts/build.ts",
        "site": "node ./scripts/build.js && parcel site/index.html",
        "test": "npm run lint && ts-node ./test/index.ts",
        "lint": "npm run eslint && npm run prettier",
        "eslint": "eslint --ext .js,.jsx,.ts --ignore-pattern 'bad.*' .",
        "prettier": "prettier -l './**/*'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlloyTeam/eslint-config-alloy.git"
    },
    "keywords": [
        "eslint",
        "eslintrc",
        "eslintconfig",
        "config",
        "alloy",
        "alloyteam",
        "javascript",
        "styleguide",
        "react",
        "vue",
        "typescript"
    ],
    "author": "xcatliu <xcatliu@gmail.com>",
    "contributors": [
        "heyli <lcxfs1991@gmail.com>",
        "DiamondYuan <admin@diamondyuan.com>",
        "Dash Chen <noreply@github.com>",
        "Swan <noreply@github.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AlloyTeam/eslint-config-alloy/issues"
    },
    "homepage": "https://alloyteam.github.io/eslint-config-alloy/",
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    },
    "dependencies": {},
    "peerDependencies": {},
    "devDependencies": {
        "@types/babylon": "^6.16.5",
        "@types/doctrine": "0.0.3",
        "@types/eslint": "^4.16.6",
        "@types/node": "^11.11.4",
        "@types/prettier": "^1.16.1",
        "@typescript-eslint/eslint-plugin": "^1.5.0",
        "babel-eslint": "^10.0.1",
        "babylon": "^6.18.0",
        "doctrine": "^3.0.0",
        "eslint": "^5.15.3",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^1.3.1",
        "mobi-plugin-color": "^1.0.0",
        "mobi.css": "^3.1.1",
        "parcel-bundler": "^1.6.2",
        "prettier": "^1.16.4",
        "prismjs": "^1.12.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "ts-node": "^7.0.1",
        "typescript": "^3.3.4000"
    }
}
